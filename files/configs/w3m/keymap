keymap   {key/quit}        QUIT
keymap   {key/command}     COMMAND
keymap C-{key/h}           HELP
keymap C-{key/suspend/low} SUSPEND
keymap   {key/clear/low}   REDRAW

keymap = LINE_INFO

keymap   {key/left}                 MOVE_LEFT
keymap   {key/down}                 MOVE_DOWN
keymap   {key/up}                   MOVE_UP
keymap   {key/right}                MOVE_RIGHT
keymap C-{key/scroll_page_up/low}   PREV_PAGE
keymap C-{key/scroll_page_down/low} NEXT_PAGE
keymap C-{key/scroll_up/low}        PREV_HALF_PAGE
keymap C-{key/scroll_down/low}      NEXT_HALF_PAGE
keymap C-{key/scroll_line_up/low}   DOWN
keymap C-{key/scroll_line_down/low} UP
keymap   {key/goto_top}             BEGIN
keymap   {key/goto_bottom}          END
keymap C-{key/g}                    GOTO_LINE
keymap   {key/scroll_line_middle}   CENTER_V
keymap   {key/line_start}           LINE_BEGIN
keymap   {key/line_end}             LINE_END
keymap   {key/word_next_start}      NEXT_WORD
keymap   {key/word_previous_start}  PREV_WORD

keymap {key/search_forward}        SEARCH_FORE
keymap {key/search_backward}       SEARCH_BACK
keymap {key/search_repeat}         SEARCH_NEXT
keymap {key/search_repeat_reverse} SEARCH_PREV

keymap {key/left/upp} BACK
keymap {key/reload}   RELOAD

keymap   {key/open}     EXTERN_LINK
keymap C-{key/open/low} EXTERN


