NORMAL                           0                   # global default
FILE                             0                   # normal file
DIR            1;{color/base09/fg}                   # directory
OTHER_WRITABLE 1;{color/base04/fg};{color/base09/bg} # directory writable by others
LINK           4;{color/base04/fg}                   # symbolic link
FIFO           4;{color/base0A/fg}                   # named pipe
SOCK           4;{color/base0B/fg}                   # socket
BLK            1;{color/base08/fg}                   # block device
CHR            4;{color/base08/fg}                   # character device
ORPHAN            {color/error/fg}                   # orphaned symlink
MISSING        1;{color/base04/fg};{color/error/bg}  # missing file that orphaned symlink points to
EXEC           1;{color/base0C/fg}                   # executable

