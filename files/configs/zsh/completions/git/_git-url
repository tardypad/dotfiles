#compdef git-url
#description display an URL related to the repo or a commit

_types() {
  local types=(
    repo
    commit
    issue
    review
  )

  compadd "$@" $types
}

_arguments \
  '-c[commit to show or get information from]:commit:__git_revisions' \
  '-r[remote to use]:remote:__git_remotes' \
  '-t[type of information]:type:_types'
