# Change current directory to a project main folder
#
# Use projects main folder given no argument

if [[ -z "$1" ]]; then
  cd "$( xdg-user-dir PROJECTS)"
  return
fi

local main_path=$( get_project_main_path "$1" )

if [[ -n "${main_path}" ]]; then
  cd "${main_path}"
else
  echo "No such main project"
fi
