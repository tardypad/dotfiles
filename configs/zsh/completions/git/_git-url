#compdef git-url
#description display an URL related to the repo or a commit

_types() {
  local types=(
    repo
    commit
    issue
    review
  )

  compadd "$@" $types
}

_arguments \
  '-c[commit to selection information]:commit:__git_revisions' \
  '-h[display the help]' \
  '-r[remote to use]:remote:__git_remotes' \
  '-t[URL type]:type:_types'
