Host *
    PubkeyAcceptedKeyTypes=+ssh-dss
    HashKnownHosts no
    EscapeChar %
    ForwardAgent no
    AddKeysToAgent yes

Host github
	Hostname github.com
	User git
	IdentityFile ~/.ssh/id_github

Host tardypad
    Hostname tardypad.me
    User damien
    Port 11235
    IdentityFile ~/.ssh/id_perso

Host status
    Hostname tardypad.me
    User damien
    Port 11235
    IdentityFile ~/.ssh/id_status

Host backup
    Hostname tardypad.me
    User backup
    Port 11235
    IdentityFile ~/.ssh/id_backup
