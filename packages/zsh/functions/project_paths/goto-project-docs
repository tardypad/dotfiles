# Change current directory to a project documents folder
#
# Use projects main documents folder given no argument

if [[ -z "$1" ]]; then
  cd "$( xdg-user-dir PROJECTS_DOCS )"
  return
fi

local docs_path=$( get-project-docs-path "$1" )

if [[ -n "${docs_path}" ]]; then
  cd "${docs_path}"
else
  echo "No such project docs"
fi
