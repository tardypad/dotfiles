# Change current directory to a project logs folder
#
# Use projects main logs folder given no argument

if [ -z "$1" ]; then
  cd "$( xdg-user-dir PROJECTS_LOGS )"
  return
fi

local logs_path="$( get-project-logs-path "$1" )"

if [ -z "${logs_path}" ]; then
  echo 'No such project logs' >&2
  return 1
fi

cd "${logs_path}"
