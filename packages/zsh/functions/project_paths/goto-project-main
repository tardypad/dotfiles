# Change current directory to a project main folder
#
# Use projects main folder given no argument

if [ -z "$1" ]; then
  cd "$( xdg-user-dir PROJECTS)"
  return
fi

local main_path="$( get-project-main-path "$1" )"

if [ -z "${main_path}" ]; then
  echo 'No such main project' >&2
  return 1
fi

cd "${main_path}"
